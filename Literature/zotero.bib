  author = {Lim, Young Kyung and Kwak, Jungwon and Kim, Dong Wook and Shin, Dongho and Yoon, Myonggeun and Park, Soah and Kim, Jin Sung and Ahn, Sung Hwan and Shin, Jungwook and Lee, Se Byeong and Park, Sung Yong and Pyo, Hong Ryeol and Kim, Dae Yong and Cho, Kwan Ho},
