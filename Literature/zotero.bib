  keywords = {*Prostheses and Implants,Benzophenones,Biomechanical Phenomena,Carbon Fiber,Carbon fiber screws,Carbon fiber spine oncology,Carbon fiber spine tumor,Humans,Polymers,Spinal Fusion/*instrumentation,Spinal Neoplasms/*surgery}
