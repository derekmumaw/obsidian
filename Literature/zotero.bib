  author = {Wolanczyk, Michal J. and Fakhrian, Khashayar and Adamietz, Irenäus A.},
  date = {2016-01-01},
  journaltitle = {Journal of Cancer},
  shortjournal = {J Cancer},
  volume = {7},
  number = {1},
