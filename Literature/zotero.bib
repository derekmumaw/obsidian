@article{wuSimultaneousIntegratedBoost2003,
  title = {Simultaneous Integrated Boost Intensity-Modulated Radiotherapy for Locally Advanced Head-and-Neck Squamous Cell Carcinomas. {{I}}: Dosimetric Results},
