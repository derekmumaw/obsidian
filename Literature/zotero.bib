  shorttitle = {Robustness Analysis for External Beam Radiotherapy Treatment Plans},
  author = {Yock, Adam D. and Mohan, Radhe and Flampouri, Stella and Bosch, Walter and Taylor, Paige and Gladstone, David and Kim, Siyong and Sohn, Jason and Wallace, Robert and Xiao, Ying and Buchsbaum, Jeff},
  date = {2019},
